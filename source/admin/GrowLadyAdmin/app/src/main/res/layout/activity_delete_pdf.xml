            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="-2dp"
            android:padding="6dp"
            android:text="Six"
            android:textAlignment="center"
            android:textColor="#151514"
            android:textSize="32sp"
            android:textStyle="bold" />

        <View
            android:id="@+id/six_view"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_below="@id/sixPdfTextView"
            android:layout_marginVertical="8dp"
            android:background="#330707"/>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/sixPdfRecycler"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/six_view"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:nestedScrollingEnabled="false"/>

        </RelativeLayout>

    <RelativeLayout
        android:id="@+id/sevenRelativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
